# Application
NODE_ENV=development
NUXT_PUBLIC_APP_NAME=Frederiksberg Kommune - een indgang
NUXT_PUBLIC_APP_URL=http://localhost:3000

# Development - Enable dev auth bypass (set to 'true' in development only)
ENABLE_DEV_AUTH=true

# Database - PostgreSQL
DATABASE_URL=postgresql://user:password@localhost:5432/frbkom_een-indgang
NUXT_DATABASE_URL=postgresql://user:password@localhost:5432/frbkom_een-indgang
DATABASE_HOST=localhost
DATABASE_PORT=5432
DATABASE_NAME=frbkom_een-indgang
DATABASE_USER=user
DATABASE_PASSWORD=password

# Authentication - Nuxt Auth Utils
NUXT_SESSION_PASSWORD=your-secret-key-min-32-characters-long

# MitID / Nets eID Broker - OIDC
NUXT_PUBLIC_NETS_CLIENT_ID=your-nets-client-id
NUXT_NETS_CLIENT_SECRET=your-nets-client-secret
NUXT_PUBLIC_NETS_ENVIRONMENT=preproduction

# SendGrid - Email
SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=noreply@frederiksberg.dk
SENDGRID_FROM_NAME=Frederiksberg Kommune

# Application Settings
TIMEZONE=Europe/Copenhagen
DEFAULT_LOCALE=da-DK
